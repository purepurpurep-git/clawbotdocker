# Copy this file to .env
#   cp .env.example .env

# Model provider API keys (fill only what you use)
OPENROUTER_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
GROQ_API_KEY=
MISTRAL_API_KEY=
CEREBRAS_API_KEY=
XAI_API_KEY=
ZAI_API_KEY=
AI_GATEWAY_API_KEY=
OPENCODE_API_KEY=
OPENCODE_ZEN_API_KEY=
MOONSHOT_API_KEY=
KIMI_API_KEY=
MINIMAX_API_KEY=
SYNTHETIC_API_KEY=
VENICE_API_KEY=
OLLAMA_API_KEY=
XIAOMI_API_KEY=

# Gateway dashboard token (required)
OPENCLAW_GATEWAY_TOKEN=change-me

# Gateway bind mode (loopback|lan|tailnet|auto|custom). Use custom for 0.0.0.0
OPENCLAW_GATEWAY_BIND=custom

# Rebuild config on each start to keep token/model in sync with .env
OPENCLAW_CONFIG_REWRITE=true

# Host port mapping for the dashboard (avoid conflicts)
HOST_PORT=18789
GATEWAY_PORT=18789

# Optional: Telegram bot token
TELEGRAM_BOT_TOKEN=

# Optional policies
TELEGRAM_DM_POLICY=pairing
TELEGRAM_GROUP_POLICY=open
TELEGRAM_REQUIRE_MENTION=false

# Optional model override
OPENCLAW_MODEL=openrouter/openai/gpt-5.2-codex

# Paths inside container
OPENCLAW_HOME=/data
OPENCLAW_WORKSPACE=/workspace
